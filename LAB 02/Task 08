#include<iostream>
using namespace std;
int main(){

    int n;
    cout<<"Enter the size of Array:";
    cin>>n;

    int *array=new int[n];
    for(int i=0;i<n;i++){
        cin>>array[i];
    }

    cout<<endl;
    cout<<"Array:";
    for(int i=0;i<n;i++){
        cout<<array[i]<<" ";
    }
    cout<<endl;

    delete[] array;

    return 0;
}

